  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "options": {
    "promisify": true
  },
  "mixins": {
    "TimeStamp": {
      "createdAt": "createdat",
      "updatedAt": "updatedat"
    }
  },
  "properties": {
    "content": {
      "type": "String"
    },
    "sender": {
      "type": "String"
    }
  },
  "acls": [
